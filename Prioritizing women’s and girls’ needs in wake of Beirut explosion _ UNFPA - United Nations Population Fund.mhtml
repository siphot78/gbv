html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video { margin: 0px; padding: 0px; border: 0px; font: inherit; vertical-align: baseline; }

article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { display: block; }

body { line-height: 1; }

ol, ul { list-style: none; }

blockquote, q { quotes: none; }

blockquote::before, blockquote::after, q::before, q::after { content: none; }

table { border-collapse: collapse; border-spacing: 0px; }

body { background: rgb(255, 255, 255); color: rgb(34, 34, 34); font-family: "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Geneva, Verdana, sans-serif; font-size: 100%; line-height: 1.5; margin: 0px; padding: 0px; }

h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6 { font-family: "Helvetica Neue", Helvetica, Arial, sans-serif; font-style: normal; font-weight: normal; }

h1, .h1 { font-size: 2em; line-height: 1.5; margin-top: 0.75em; }

h2, .h2 { font-size: 1.5em; line-height: 1.5; margin-top: 1em; }

h3, .h3 { font-size: 1em; line-height: 1.5; margin-top: 1.25em; }

h4, h5, h6, .h4, .h5, .h6 { font-size: 1em; font-weight: bold; line-height: 1.5; margin-top: 1.5em; }

p { font-size: 1em; line-height: 1.5; margin: 0px 0px 1.5em; }

a { color: rgb(28, 151, 200); text-decoration: none; }

a:visited { color: rgb(28, 151, 200); }

a:hover, a:active, li a.active { color: rgb(48, 179, 230); }

a:focus { outline: dotted thin; }

a:hover, a:active { outline: 0px; }

li ul, li ol { margin: 0px; }

ul, ol { margin: 0px 1.5em 1.5em 0px; padding-left: 1.5em; }

ul { list-style-type: disc; }

ol { list-style-type: decimal; }

dl { margin: 0px 0px 1.5em; }

dl dt { font-weight: bold; }

dd { margin-left: 1.5em; }

hr { background: rgb(102, 102, 102); border: none; clear: both; float: none; height: 1px; margin: 0px 0px 1.4375em; width: 100%; }

fieldset { border: 1px solid rgb(204, 204, 204); margin: 0px 0px 1.4375em; padding: 1.5em; }

table { margin-bottom: 1.5em; width: 100%; }

th, tr, td { vertical-align: middle; }

th { padding-right: 1em; }

.sticky-header th, .sticky-table th { border-bottom: 3px solid rgb(204, 204, 204); padding-right: 1em; text-align: left; }

#container { padding: 0px 10px; }

#main { display: inline; margin: 0px; }

body.two-sidebars #main { float: left; margin: 0px 0px 0px 240px; padding: 0px; width: 460px; }

body.sidebar-first #main { float: right; margin: 0px; padding: 0px; width: 700px; }

body.sidebar-second #main { float: left; margin: 0px; padding: 0px; width: 700px; }

body.two-sidebars #sidebar-first { float: left; margin: 0px 0px 0px -700px; padding: 0px; width: 220px; }

body.two-sidebars #sidebar-second { float: right; margin: 0px; padding: 0px; width: 220px; }

body.sidebar-first #sidebar-first { float: left; margin: 0px; padding: 0px; width: 220px; }

body.sidebar-second #sidebar-second { float: right; margin: 0px; padding: 0px; width: 220px; }

#header { margin: 0px; padding: 2.5em 0px 0px; position: relative; }

#header #logo { float: left; margin: 0px 1em 1.5em 0px; }

#header #site-name-slogan { float: left; margin: 0px 0px 1.5em; }

#header #site-name { font-family: Arial, Helvetica, sans-serif; font-size: 2.5em; font-weight: 700; line-height: 1.2; margin: 0px; }

#header #site-name a:hover { text-decoration: none; }

#header #site-slogan { margin: 0px; }

#navigation { clear: both; margin: 0px; padding: 0px; }

#navigation ul#main-menu { clear: both; list-style: none; margin: 0px; padding: 0px; }

#navigation ul#main-menu li { float: left; margin: 0px; padding: 0px; }

#navigation ul#main-menu li a { display: block; padding: 0px 2em 0px 0px; }

#navigation ul#secondary-menu { clear: both; list-style: none; margin: 0px; padding: 0px; }

#navigation ul#secondary-menu li { float: left; margin: 0px; padding: 0px; }

#navigation ul#secondary-menu li a { display: block; padding: 0px 2em 0px 0px; }

.breadcrumb { clear: both; padding: 1.5em 0px 0px; }

#skip-link { position: absolute; }

.item-list ul { margin: 0px; padding: 0px 0px 0px 1.5em; }

.item-list ul li { margin: 0px; padding: 0px; }

ul.menu li { margin: 0px; padding: 0px; }

ul.inline { clear: both; }

ul.inline li { margin: 0px; padding: 0px 1em 0px 0px; }

.tabs-wrapper { border-bottom: 1px solid rgb(51, 51, 51); margin: 0px 0px 1.4375em; position: relative; z-index: 3; background-color: rgb(255, 255, 255); }

ul.primary { border: none; margin: 0px; padding: 0px; }

ul.primary li a { background: none; border: none; display: block; float: left; line-height: 1.5em; margin: 0px; padding: 0px 1em; }

ul.primary li a:hover, ul.primary li a.active { background: rgb(102, 102, 102); border: none; color: rgb(255, 255, 255); }

ul.primary li a:hover { background: rgb(136, 136, 136); text-decoration: none; }

ul.secondary { background: rgb(102, 102, 102); border-bottom: none; clear: both; margin: 0px; padding: 0px; }

ul.secondary li { border-right: none; }

ul.secondary li a, ul.secondary li a:link { border: none; color: rgb(204, 204, 204); display: block; float: left; line-height: 1.5em; padding: 0px 1em; }

ul.secondary li a:hover, ul.secondary li a.active { background: rgb(136, 136, 136); color: rgb(255, 255, 255); text-decoration: none; }

ul.secondary a.active { border-bottom: none; }

.node { margin-bottom: 1.5em; }

.field-name-field-tags { margin: 0px 0px 1.5em; }

.field-name-field-tags .field-item { margin: 0px 1em 0px 0px; }

.field-name-field-tags div { display: inline; }

.profile { margin: 1.5em 0px; }

.profile h3 { border: none; }

.profile dt { margin-bottom: 0px; }

.profile dd { margin-bottom: 1.5em; }

.password-parent { width: 36em; }

#comments { clear: both; margin-top: 1.5em; }

.comment { margin-bottom: 1.5em; }

.comment .new { color: red; text-transform: capitalize; margin-left: 1em; }

.block { margin-bottom: 1.5em; }

#footer { clear: both; padding: 1.5em 0px; position: relative; }

#footer a.feed-icon { position: absolute; right: 0px; top: 1.6667em; }

.clear { clear: both; }

.display-block { display: block; }

.news-img { float: left; padding-right: 5px; }

.news-home-list { color: rgb(107, 106, 106); }

.news-home-list .views-row, .event-home-list .views-row, .event-list .views-row { border-bottom: 1px solid rgb(204, 204, 204); padding: 15px 0px; }

.pane-vw-news .pane-title { border-bottom: 1px solid rgb(204, 204, 204); padding-bottom: 10px; }

.pane-vw-events .pane-title, .pane-vw-publications .pane-title { border-bottom: 1px solid rgb(204, 204, 204); padding-bottom: 10px; }

.pane-vw-video .pane-title { border-bottom: 1px solid rgb(204, 204, 204); padding-bottom: 10px; }

.event-date { color: rgb(223, 122, 14); font-weight: bold; background: url("/sites/all/themes/unfpa_global/images/calender-icon.png") 0px 0px no-repeat; padding: 0px 5px 0px 25px; }

.view-vw-publication-field-collection-view .views-row { border-bottom: none !important; }

